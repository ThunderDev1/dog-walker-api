{
  "ConnectionStrings": {
    "DefaultConnection": "Server=(localdb)\\mssqllocaldb; Database=DogWalker; Trusted_Connection=True; MultipleActiveResultSets=true"
  },
  "Endpoints":{
    "Spa": "http://localhost:5100",
    "Identity": "http://localhost:5000"
  },
  "Storage": {
    "ConnectionString": "UseDevelopmentStorage=true",
    "MaxUploadSizeMb": 8,
    "AuthorizedImageFormats": [".png", ".gif", ".jpg", ".jpeg"]
  },
  "Logging": {
    "IncludeScopes": false,
    "Debug": {
      "LogLevel": {
        "Default": "Warning"
      }
    },
    "Console": {
      "LogLevel": {
        "Default": "Warning"
      }
    }
  }
}
